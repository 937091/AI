a = []
n = int(input("Enter the number of elements: "))

for i in range(n):
    element = int(input("Enter the element: "))
    a.append(element)

def selectionsort(a):
    for i in range(len(a) - 1):
        min_index = i
        for j in range(i + 1, len(a)):
            if a[min_index] > a[j]:
                min_index = j
        a[i], a[min_index] = a[min_index], a[i]

print("Unsorted array:", a)
selectionsort(a)
print("Sorted array:", a)
